{
    "multipart": [
        {
            "when": { "facing": "north", "state": "true" },
            "apply": { "model": "projectsteam_crafting:block/woodmill", "y": 90 }
        },
        {
            "when": { "facing": "south" , "state": "true"},
            "apply": { "model": "projectsteam_crafting:block/woodmill", "y": 270 }
        },
        {
            "when": { "facing": "west" , "state": "true"},
            "apply": { "model": "projectsteam_crafting:block/woodmill", "y": 0 }
        },
        {
            "when": { "facing": "east" , "state": "true"},
            "apply": { "model": "projectsteam_crafting:block/woodmill", "y": 180 }
        },

        {
            "when": { "facing": "north", "state": "false" },
            "apply": { "model": "projectsteam_crafting:block/woodmill_master", "y": 90 }
        },
        {
            "when": { "facing": "south" , "state": "false"},
            "apply": { "model": "projectsteam_crafting:block/woodmill_master", "y": 270 }
        },
        {
            "when": { "facing": "west" , "state": "false"},
            "apply": { "model": "projectsteam_crafting:block/woodmill_master", "y": 0 }
        },
        {
            "when": { "facing": "east" , "state": "false"},
            "apply": { "model": "projectsteam_crafting:block/woodmill_master", "y": 180 }
        }
    ]
}
